{
  "name": "back",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "prebuild": "npm run lint",
    "build": "tsc && npm run sentry:sourcemaps",
    "start": "node dist/index.js",
    "dev": "nodemon --watch src --ext ts --exec ts-node src/index.ts",
    "prestart": "npm run build",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "format": "prettier . --write",
    "db:generate": "prisma generate",
    "db:migrate": "prisma migrate dev",
    "db:migrate:deploy": "prisma migrate deploy",
    "db:studio": "prisma studio",
    "db:dev": "prisma dev",
    "test": "jest",
    "sentry:sourcemaps": "sentry-cli sourcemaps inject --org felipe-5h --project node-express ./dist && sentry-cli sourcemaps upload --org felipe-5h --project node-express ./dist"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "@prisma/client": "^6.11.1",
    "@prisma/extension-accelerate": "^2.0.1",
    "@sentry/cli": "^2.58.2",
    "@sentry/node": "^10.25.0",
    "@sentry/profiling-node": "^10.25.0",
    "bcrypt": "^6.0.0",
    "dotenv": "^16.5.0",
    "express": "^5.1.0",
    "jsonwebtoken": "^9.0.3",
    "zod": "^3.25.74"
  },
  "devDependencies": {
    "@eslint/js": "^9.29.0",
    "@jest/globals": "^29.7.0",
    "@types/bcrypt": "^6.0.0",
    "@types/express": "^5.0.3",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^24.0.1",
    "@typescript-eslint/eslint-plugin": "^8.34.0",
    "@typescript-eslint/parser": "^8.34.0",
    "eslint": "^9.29.0",
    "eslint-plugin-node": "^11.1.0",
    "globals": "^16.2.0",
    "jest": "^29.7.0",
    "nodemon": "^3.1.10",
    "prettier": "3.5.3",
    "prisma": "^6.11.1",
    "ts-jest": "^29.4.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.34.0"
  }
}
